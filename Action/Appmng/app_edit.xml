<html>
<head>
<LINK REL="stylesheet" type="text/css" HREF="[CORE_SBASEURL]app=CORE&action=CORE_CSS" >

[JS:REF]
<script language="JavaScript">
<!--
[JS:CODE]
//-->
</script>

</head>

<body>

<form name="edit" method="POST">
<input type="hidden" name="id" value="[id]">

<table align="center"  border="0" cellspacing="1" cellpadding="1">
<tr>
<td class="CORETblHeadCell">

  <table align="center"  border="0" cellspacing="0" cellpadding="2">
  <tr>
    <td colspan="3" align="center" class="CORETblHeadCell"><span class="CORETblHead">
    [TITRE] 
    </span></td>
  </tr>
  <tr>
    <td colspan="3" align="left" class="CORETblCell">&nbsp;</td>
  </tr>
  
  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:name] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" size="25" name="name" value="[name]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:short_name] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" size="25" name="short_name" value="[short_name]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:description] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" size="25" name="description" value="[description]"></font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:access_free] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="access_free" SIZE=1>
      [BLOCK SELECTACCESS]

        <option value="[access_free]" [selected]>[access_free]</option>

      [ENDBLOCK SELECTACCESS]

    </SELECT
    </span></td>
  </tr>
  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:appssl] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="ssl" SIZE=1>
      [BLOCK SELECTSSL]

        <option value="[ssl]" [selected]>[ssl]</option>

      [ENDBLOCK SELECTSSL]

    </SELECT
    </span></td>
  </tr>
  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:available] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="available" SIZE=1>
      [BLOCK SELECTAVAILABLE]

        <option value="[available]" [selected]>[available]</option>

      [ENDBLOCK SELECTAVAILABLE]

    </SELECT
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:displayable] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
    <SELECT NAME="displayable" SIZE=1>
      [BLOCK SELECTDISPLAYABLE]

        <option value="[displayable]" [selected]>[displayable]</option>

      [ENDBLOCK SELECTDISPLAYABLE]

    </SELECT
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:machine] : </span></td>
    <td colspan="2" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1"><input type="text" size="25" name="machine" value="[machine]"></font>
    </span></td>
  </tr>
  <tr>
    <td colspan="3" align="left" class="CORETbCell">&nbsp;</td>
  </tr>

  <tr>

    <td colspan="3" align="center" class="CORETblHeadCell">
      <font size="-1"><input class="CORETblHead" type="reset" onClick="self.close()" name="Annuler" value="[TEXT:butclosew]">
      &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <input class="CORETblHead" type="reset" 
        onClick="return Valid_Send()" name="Creer" value="[BUTTONTYPE]">
      </font>
    </td>

  </tr>
  </table>

</td>
</tr>
</table>
</form>
</body>
</head>
