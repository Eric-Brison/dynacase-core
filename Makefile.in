# ============================================
PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @PACKAGE@
pubdir = @prefix@
srcdir = @srcdir@
pidir = /var/post-install/bin

export pubdir utildir appname pidir

TAR = gtar
GZIP_ENV = --best



export targetdir
export utildir

SUBDIR= Class Images Action Zone App Api Share Doc Style

TOP_MODULES = index.php wsh.php  dbaccess.php apache.conf  whattext wcheck wstop


include $(utildir)/PubRule

DISTFILES = $(SUBDIR) configure.in  \
            Makefile.in index.php wsh.php  dbaccess.php.in \
            apache.conf.in WHAT.spec.in ChangeLog  VERSION RELEASE \
	    $(PACKAGE)_en.po $(PACKAGE)_fr.po whattext wcheck wstop


PIFILES = $(PACKAGE)_post.sh

$(pubdir)/whattext: whattext
	cp $< $@
$(pubdir)/wcheck: wcheck
	cp $< $@
$(pubdir)/wstop: wstop
	cp $< $@
