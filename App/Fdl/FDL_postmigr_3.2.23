#!/bin/bash

set -e

if [ -z "$wpub" ]; then
    echo "Undefined or empty wpub environment variable!"
    exit 1
fi

if [ -z "$pgservice_core" ]; then
    echo "Undefined or empty pgservice_core!"
    exit 1
fi


"$wpub"/wsh.php --api=importDocuments --file="$wpub/FDL/cv_init.csv" --csv-separator=';' --csv-enclosure='"'


exit 0
