#!/bin/bash

if [ -z "$pgservice_freedom" ]; then
    echo "Undefined or empty pgservice_freedom!"
    exit 1
fi

PGSERVICE="$pgservice_freedom" psql --set ON_ERROR_STOP=on <<EOF 2>&1
CREATE INDEX vault_teng on vaultdiskstorage (teng_state);
create index in_doclog on doclog(initid);
EOF

RET=$?
if [ $RET -ne 0 ]; then
    echo "Error: SQL error in `basename $0`"
    exit $RET
fi

$wpub/wsh.php --api=cleanFileName

exit 0
