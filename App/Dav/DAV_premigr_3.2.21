#!/bin/bash

if [ -z "$pgservice_core" ]; then
	echo "Undefined or empty pgservice_core!"
	exit 1
fi

PGSERVICE="$pgservice_core" psql --set ON_ERROR_STOP=on -f - <<'EOF'
DELETE FROM paramv USING application
    WHERE paramv.appid = application.id AND application.name = 'DAV' AND paramv.name = 'WEBDAV_DB' RETURNING paramv.*;
EOF
RET=$?
if [ $RET -ne 0 ]; then
    echo "Error removing WEBDAV_DB parameter."
    exit $RET
fi
