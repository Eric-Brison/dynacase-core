Alias /what @prefix@
Alias /freedom @prefix@


<Directory "@prefix@">
    AddType application/x-httpd-php .php
    DirectoryIndex  index.php
    AllowOverride Options
    <Files ~ "index.php">
      ErrorDocument 401 /what/expire/tryexpire.html
      <IfModule mod_authn_pam.c>
        AuthType Basic
        AuthName 'FREEDOM Connection'      
        AuthBasicProvider pam
        AuthnPAMService httpd
        Require valid-user
      </IfModule>
    </Files>
    <Files ~ "indexq.php">
      ErrorDocument 401 /what/expire/tryexpire.html
      <IfModule mod_authn_pam.c>
        AuthType Basic
        AuthName 'FREEDOM Connection'      
        AuthBasicProvider pam
        AuthnPAMService httpd
        Require valid-user
      </IfModule>
    </Files>
    <FilesMatch "\.conf|dbaccess.php|wstop|wcheck|wcontrol|wsh.php">
       Order Deny,Allow
       Deny from All   
    </FilesMatch>
    <FilesMatch "\.(gif|jpe?g|png|xpm|ico|js|html|css)$">
       Order Deny,Allow
       Allow from All   
    </FilesMatch>
    <FilesMatch "geticon.php">
      Allow from All   
    </FilesMatch>
    <FilesMatch "expire.shtml|chgpasswd.php">
      Allow from All
      <IfModule mod_authn_pam.c>
        AuthType Basic
        AuthName 'FREEDOM Connection'
        AuthBasicProvider pam
        AuthnPAMService httpdwa
        Require valid-user
      </IfModule>
    </FilesMatch>
    ExpiresActive On
    ExpiresByType application/x-javascript  A31536000
</Directory>



 <Directory ~ @prefix@/[A-Z]>
     Order Deny,Allow
     Deny from All   
 </Directory> 

<Directory "@prefix@/expire/">
    ErrorDocument 401 "connection refused"   
    Options IncludesNoExec FollowSymLinks 
</Directory>
<Directory "@prefix@/admin/">
    DirectoryIndex  winit.php
    php_value session.name "adminsession"
    php_value session.use_cookies 1
    AuthType Basic
    AuthName "FREEDOM Administration"
    AuthUserFile @prefix@/admin/.htpasswd
    Require valid-user
</Directory>

