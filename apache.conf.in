Alias /what @prefix@

<Directory "@prefix@">
    php_value include_path ".:@prefix@:@prefix@/@PACKAGE@:/home/httpd/advGraph"
    php_value session.use_cookies 0
    php_value session.name "session"
    php_value session.use_trans_sid 0
    AddType application/x-httpd-php .php
    DirectoryIndex  index.php
    <Files ~ "index.php">
      AuthType Basic
      AuthName 'WHAT Connection'
      AuthPAM_Enabled  on
      AuthPAM_FallThrough off
      require valid-user
    </Files>
    <FilesMatch "\.conf|dbaccess.php|wstop|wcheck|wsh.php">
       Order Deny,Allow
       Deny from All   
    </FilesMatch>
</Directory>


 <DirectoryMatch /home/httpd/what/[^CALENDAR].*>
     Order Deny,Allow
     Deny from All   
    <FilesMatch "\.(gif|jpe?g|png|xpm|ico|js|html|css)$">
       Order Deny,Allow
       Allow from All   
    </FilesMatch>
    <FilesMatch "geticon.php">
      Allow from All   
      AuthType Basic
      AuthName 'WHAT Connection'
      AuthPAM_Enabled  on
      AuthPAM_FallThrough off
      require valid-user
    </FilesMatch>

 </DirectoryMatch> 

 <Directory /home/httpd/what/CALENDAR>
    AuthType Basic
    AuthName 'WHAT Connection'
    AuthPAM_Enabled  on
    AuthPAM_FallThrough off
    require valid-user

 </Directory>

