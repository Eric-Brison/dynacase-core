Alias /what @prefix@
Alias /freedom @prefix@


<Directory "@prefix@">
    php_value include_path ".:@prefix@:@prefix@/WHAT:/usr/share/pear"
    php_value session.use_cookies 1
    php_value session.name "session"
    php_value session.use_trans_sid 0
    php_value post_max_size "80M"
    php_value upload_max_filesize "80M"
    php_value default_charset  "iso-8859-1"
    AddType application/x-httpd-php .php
    DirectoryIndex  index.php
    <Files ~ "index.php">
      ErrorDocument 401 /what/expire/tryexpire.html
      AuthType Basic
      AuthName 'FREEDOM Connection'
      AuthPAM_Enabled  on
      AuthPAM_FallThrough off
      AuthPAM_Service httpd
      Require valid-user
    </Files>
    <FilesMatch "\.conf|dbaccess.php|wstop|wcheck|wsh.php">
       Order Deny,Allow
       Deny from All   
    </FilesMatch>
    <FilesMatch "\.(gif|jpe?g|png|xpm|ico|js|html|css)$">
       Order Deny,Allow
       Allow from All   
    </FilesMatch>
    <FilesMatch "geticon.php">
      Allow from All   
      AuthType Basic
      AuthName 'FREEDOM Connection'
      AuthPAM_Enabled  on
      AuthPAM_FallThrough off
      Require valid-user
    </FilesMatch>
    <FilesMatch "expire.shtml|chgpasswd.php">
      Allow from All   
      AuthType Basic
      AuthName 'FREEDOM Connection'
      AuthPAM_Enabled  on
      AuthPAM_FallThrough off
      AuthPAM_Service httpdwa
      Require valid-user
    </FilesMatch>
    ExpiresActive On
    ExpiresByType application/x-javascript  A31536000
</Directory>



 <Directory ~ @prefix@/[A-B,D-Z]|@prefix@/C[^A]>
     Order Deny,Allow
     Deny from All   
 </Directory> 

<Directory "@prefix@/expire/">
    ErrorDocument 401 "connection refused"   
    Options IncludesNoExec FollowSymLinks 
</Directory>
<Directory "@prefix@/admin/">
    DirectoryIndex  index.html     
    php_value session.name "adminsession"
    AuthType Basic
    AuthName "FREEDOM Administration"
    AuthUserFile @prefix@/admin/.htpasswd
    Require valid-user
</Directory>


 <Directory @prefix@/CALENDAR>
    AllowOverride None
    ErrorDocument 401 "connection refused"   
    AuthType Basic
    AuthName 'FREEDOM Connection'
    AuthPAM_Enabled  on
    AuthPAM_FallThrough off
    require valid-user
 </Directory>


Alias /whatsync @prefix@/CALENDAR
